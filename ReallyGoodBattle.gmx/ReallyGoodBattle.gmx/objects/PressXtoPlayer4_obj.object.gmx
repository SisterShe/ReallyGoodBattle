        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handles Input

//Check if disabled and ai.
if (disabled and global.player4_ai)
{
    if (input_check_pressed(index, ord("4")))
    {
        //Updates ai fighter.
        with (Player4_obj)
        {
            //Updates fighter for player.
            self.fighter_index += 1;
            if (self.fighter_index &gt; ds_list_size(obj_fighter_container.list) - 1)
            {
                self.fighter_index = 0;
            }
            self.fighter = ds_list_find_value(obj_fighter_container.list, self.fighter_index);
            event_user(0);
            
            //Updates game master.
            with (GameMaster_obj)
            {
               Player4Red = other.fighter.red;
               Player4Green = other.fighter.green;
               Player4Blue = other.fighter.blue;
               Player4Fighter = other.fighter;
            }
            
            //Plays popup animation for name.
            var name = self.fighter.name;
            animation_text_popup(-100, 1, 0.02, fnt_joystix_8, fa_center,
            fa_top, x, y, name, 16, 100, room_seconds(2), 30, 90, c_white, c_white);
        }
    }
}

//Check if disabled.
if (disabled) exit;

//Check for Join input.
if (input_check_pressed(index, key1, key2, key3, key4)) 
{
     instance_create(Player4Spawn_obj.x,Player4Spawn_obj.y,Player4_obj);
     with (Player4_obj)
     {
        self.fighter_index = irandom_range(0, ds_list_size(obj_fighter_container.list) - 1);
        self.fighter = ds_list_find_value(obj_fighter_container.list, self.fighter_index);
        event_user(0);
        with (GameMaster_obj)
        {
           Player4Red = other.fighter.red;
           Player4Green = other.fighter.green;
           Player4Blue = other.fighter.blue;
           Player4Fighter = other.fighter;
           Player4Active = 1;
           Player4Ready = 0;
        }
     }
     instance_create(384,224,Player4ReadyTexts); 
     instance_create(336,176,Ready4Up_obj); 
     instance_create(432,176,Recolor4_obj);
     instance_destroy();
     audio_play_sound_volume(sfx_rgb_up, 10, false, global.sfxvolume); 
}

//Check for AI input.
if (input_check_pressed(index, ord("4")))
{
    //Disable
    disabled = true;
    global.player4_ai = true;
    audio_play_sound_volume(sfx_rgb_up, 10, false, global.sfxvolume);
    var fighter_index = irandom_range(0, ds_list_size(obj_fighter_container.list) - 1);
    var fighter = ds_list_find_value(obj_fighter_container.list, fighter_index);
    with (GameMaster_obj)
    {
       Player4Red = fighter.red;
       Player4Green = fighter.green;
       Player4Blue = fighter.blue;
       Player4Fighter = fighter;
    }
    var ai = instance_create(Player4Spawn_obj.x, Player4Spawn_obj.y, Player4_obj);
    with (ai)
    {
        self.fighter_index = fighter_index;
        self.fighter = fighter;
        event_user(0);
    }
}
</string>
