            <string>///Handles Drawing.

//Check if correct room.
if (room = rm_mainmenu or room = rm_gamesetup or room = rm_pause) exit;

//Runs code from game master.
with (GameMaster_obj)
{
    //Setup Drawing.
    var red = Player2Red;
    var green = Player2Green;
    var blue = Player2Blue;
    var color = make_color_rgb(red, green, blue);
    draw_set_color(color);
    var xx = other.x;
    var yy = other.y
    
    //Determine circle radius.
    var pct = alarm[1] / RespawnTimer;
    var radius_full = 8;
    var radius = radius_full * pct;
    
    //Check if player doesn't exists.
    if (!instance_exists(Player2_obj))
    {
        //Check if player is AI.
        if (global.player2_ai)
        {
            //Draws Respawn Effect
            draw_circle(xx, yy, radius, false);
        }
        
        //Check if player is active.
        else if (Player2Active)    
        {
            //Draws Respawn Effect
            draw_circle(xx, yy, radius, false);
        }
    }
}

