          <argument>
            <kind>1</kind>
            <string>///Handles Input

//Detects input.
Input_Script(1); 

//Checks for movement.
var move = 0; 
move = (right_key - left_key); 
if (move != 0 and !input_delay) 
{
    //Clamps movement inside bounds.
    mpos += move;
    if (mpos &lt; 0) mpos = array_length_1d(RGB) - 1; 
    if (mpos &gt; array_length_1d(RGB) - 1) mpos = 0;
    input_delay = true;
    alarm[0] = input_delay_duration;
    
    //Check for color
    switch (mpos) {
        case 0: {
            audio_play_sound_volume(sfx_select_red, 10, false, global.sfxvolume);
            break;
        }
         case 1: {
            audio_play_sound_volume(sfx_select_green, 10, false, global.sfxvolume);
            break;
        }
         case 2: {
            audio_play_sound_volume(sfx_select_blue, 10, false, global.sfxvolume);
            break;
        }
    }  
}

//Check for push input.
var push; 
push = (up_key - down_key);
if (push = 1) {menuUp_scr();}
if (push = -1) {menuDown_scr();}

//Clamps RGB values between 0-255.
Rval = clamp(Rval, 0, 255);
Gval = clamp(Gval, 0, 255);
Bval = clamp(Bval, 0 ,255);

//Creates player object.
if (jump_key_pressed) 
{
     with (GameMaster_obj)
     {
        Player2Red = other.Rval;
        Player2Green = other.Gval;
        Player2Blue = other.Bval;
     }
     instance_create(Player2Spawn_obj.x,Player2Spawn_obj.y,Player2_obj);
     instance_create(384,64,Player2ReadyTexts); 
     instance_create(336,48,Ready2Up_obj); 
     instance_create(416,48,Recolor2_obj);
     instance_create(384, 112, obj_sprite_selector_p2);
     instance_destroy();
}

//Returns to Press X to Join 
if (attack_key_tap) 
{
    instance_create(x+72,y+32,PressXtoPlayer2_obj);
    instance_destroy(); 
}
</string>
