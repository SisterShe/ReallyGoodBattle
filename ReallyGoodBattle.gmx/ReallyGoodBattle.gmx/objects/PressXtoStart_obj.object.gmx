          <argument>
            <kind>1</kind>
            <string>///Handles input and RGB effect.

//Detects input.
Input_Script(0);

//Detects timer changes.
timer = timer + 1; 
if (timer &lt; 60) {
   Color = choose(c_red,c_green,c_blue); 
   alpha = 0; 
}
if (timer &gt; 60) {
    alpha = 1;  
}
if (timer &gt;= 160) {
    timer = 0
}

//Goes to main menu.
if (input_check_pressed(0, gp_face1, vk_enter, ord("E"), vk_escape, vk_space)) 
{
    room_cfade(rm_mainmenu, room_seconds(0.5), c_black);
    
    //stops music.
    audio_sound_gain(global.music, 0, room_seconds(0.5));
    
    //Sets music for menu.
    global.music = audio_play_sound_volume(msi_menu, 5, true, global.musicvolume);
    
    audio_play_sound_volume(sfx_ready_up, 10, false, global.sfxvolume);
}
</string>
